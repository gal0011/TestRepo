{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off",
        "image-alt": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "meta-charset-utf-8": "off",
    "axe/aria": [
      "default",
      {
        "aria-progressbar-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "compat-api/css": "off",
    "css-prefix-order": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 16.3",
    "not safari <= 16.3"
  ]
}